{
    "name": "octraves/profile-piler-api",
    "description": "Profile Piler API",
    "keywords": [
        "API",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "aws/aws-sdk-php": "^3.133",
        "elasticsearch/elasticsearch": "^7.5",
        "flipbox/lumen-generator": "^6.0",
        "fruitcake/laravel-cors": "^1.0",
        "jsq/amazon-es-php": "^0.3.0",
        "laravel/lumen-framework": "^8.0",
        "paquettg/php-html-parser": "^2.2",
        "php-curl-class/php-curl-class": "^8.6",
        "wizacha/aws-signature-middleware": "^0.1.2"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
